on early-init
    mkdir /mnt/vendor/efs 0771 radio system
    mkdir /mnt/vendor/cpefs 0771 radio system
    mkdir /mnt/vendor/persist 0771 system system
    mount debugfs /sys/kernel/debug /sys/kernel/debug
    exec u:r:vendor_modprobe:s0 -- /vendor/bin/modprobe -a -d /vendor/lib/modules input_booster_lkm.ko
    export MEMTAG_OPTIONS off

# fingerprintdriver, fingerprint, fingerprint_sysfs qbt2000_spidev
    exec u:r:vendor_modprobe:s0 -- /vendor/bin/modprobe -a -d /vendor/lib/modules fingerprint.ko fingerprint_sysfs.ko qbt2000_spidev.ko

# sec_debug
    exec u:r:vendor_modprobe:s0 -- /vendor/bin/modprobe -a -d /vendor/lib/modules sec_debug_coredump.ko

on init
    # See storage config details at http://source.android.com/tech/storage/
    mkdir /storage/sdcard 0000 root root
    mkdir /storage/usb1 0000 root root
    mkdir /storage/usb2 0000 root root
    mkdir /mnt/media_rw/sdcard 0700 media_rw media_rw
    mkdir /mnt/media_rw/usb1 0700 media_rw media_rw
    mkdir /mnt/media_rw/usb2 0700 media_rw media_rw

    export SECONDARY_STORAGE /storage/sdcard:/storage/usb1:/storage/usb2

    # Support legacy paths
    symlink /storage/sdcard /mnt/ext_sd
    symlink /data/app /factory

    symlink /dev/block/by-name/steady /dev/block/steady
    symlink /dev/block/by-name/persistent /dev/block/persistent
    symlink /dev/block/by-name/param /dev/block/param

    chown root system /sys/class/sec/gpio/check_requested_gpio
    chmod 0664 /sys/class/sec/gpio/check_requested_gpio

    # GFX
    chown system system /sys/kernel/asv/power_model_0
    chown system system /sys/kernel/asv/power_model_1
    chown system system /sys/kernel/asv/power_model_2
    chown system system /sys/kernel/asv/power_model_3

    chown system system /sys/kernel/asv/mcd_ver
    chown system system /sys/kernel/asv/mht_ver

    # Change CPUFreq governor to energy_aware(EGO)
    write /sys/devices/system/cpu/cpufreq/policy0/scaling_governor energy_aware
    write /sys/devices/system/cpu/cpufreq/policy4/scaling_governor energy_aware
    write /sys/devices/system/cpu/cpufreq/policy7/scaling_governor energy_aware

    # Change CPUIdle governor to TEO
    write /sys/devices/system/cpu/cpuidle/current_governor teo

    # Uclamp.min configuration
    chown root system /dev/cpuctl/top-app/cpu.uclamp.min
    chmod 0664 /dev/cpuctl/top-app/cpu.uclamp.min

    # HyPer Sysfs
    chown root system /sys/class/devfreq/17000010.devfreq_mif/scaling_devfreq_max
    chmod 0660 /sys/class/devfreq/17000010.devfreq_mif/scaling_devfreq_max

    chown root system /sys/devices/system/cpu/cpufreq_limit/little_min_limit
    chmod 0664 /sys/devices/system/cpu/cpufreq_limit/little_min_limit

    # Clear sched_util_clamp_min_rt_default
    write /proc/sys/kernel/sched_util_clamp_min_rt_default 0

    # EMS interfaces
    chown system system /dev/ems
    chown system system /sys/kernel/ems/emstune/req_mode
    chown system system /sys/kernel/ems/emstune/req_level
    chown system system /sys/kernel/ems/emstune/aio_tuner
    chown system system /sys/kernel/ems/emstune/reset
    chown system system /sys/kernel/ems/emstune/sched_boost
    chown system system /sys/kernel/ems/emstune/task_boost
    chown system system /sys/kernel/ems/energy_step/coregroup0/uclamp_min
    chown system system /sys/kernel/ems/energy_step/coregroup4/uclamp_min
    chown system system /sys/kernel/ems/energy_step/coregroup7/uclamp_min
    chown system system /sys/kernel/ems/energy_step/coregroup0/uclamp_max
    chown system system /sys/kernel/ems/energy_step/coregroup4/uclamp_max
    chown system system /sys/kernel/ems/energy_step/coregroup7/uclamp_max
    chown system system /sys/kernel/ems/energy_step/coregroup0/uclamp_busy_ratio
    chown system system /sys/kernel/ems/energy_step/coregroup4/uclamp_busy_ratio
    chown system system /sys/kernel/ems/energy_step/coregroup7/uclamp_busy_ratio
    chown system system /sys/kernel/ems/energy_step/coregroup0/uclamp_monitor_len
    chown system system /sys/kernel/ems/energy_step/coregroup4/uclamp_monitor_len
    chown system system /sys/kernel/ems/energy_step/coregroup7/uclamp_monitor_len
    chown system system /sys/kernel/ems/ego/coregroup0/uclamp_min
    chown system system /sys/kernel/ems/ego/coregroup4/uclamp_min
    chown system system /sys/kernel/ems/ego/coregroup7/uclamp_min
    chown system system /sys/kernel/ems/ego/coregroup0/uclamp_max
    chown system system /sys/kernel/ems/ego/coregroup4/uclamp_max
    chown system system /sys/kernel/ems/ego/coregroup7/uclamp_max
    chown system system /sys/kernel/ems/ego/coregroup0/uclamp_busy_ratio
    chown system system /sys/kernel/ems/ego/coregroup4/uclamp_busy_ratio
    chown system system /sys/kernel/ems/ego/coregroup7/uclamp_busy_ratio
    chown system system /sys/kernel/ems/ego/coregroup0/uclamp_monitor_len
    chown system system /sys/kernel/ems/ego/coregroup4/uclamp_monitor_len
    chown system system /sys/kernel/ems/ego/coregroup7/uclamp_monitor_len
    chown system system /sys/kernel/ems/gsc/min_admit_ns

    # EAS stune boosting interfaces
    chown system system /dev/stune/top-app/schedtune.boost
    chown system system /dev/stune/top-app/schedtune.prefer_idle
    chown system system /dev/stune/top-app/schedtune.prefer_perf
    chown system system /dev/stune/top-app/cgroup.procs

    chown system system /dev/stune/foreground/schedtune.boost
    chown system system /dev/stune/foreground/schedtune.prefer_idle
    chown system system /dev/stune/foreground/schedtune.prefer_perf
    chown system system /dev/stune/foreground/cgroup.procs

    chown system system /dev/stune/background/schedtune.boost
    chown system system /dev/stune/background/schedtune.prefer_idle
    chown system system /dev/stune/background/schedtune.prefer_perf
    chown system system /dev/stune/background/cgroup.procs

    chown system system /dev/stune/schedtune.boost
    chown system system /dev/stune/schedtune.prefer_idle
    chown system system /dev/stune/schedtune.prefer_perf
    chown system system /dev/stune/cgroup.procs

    write /dev/stune/top-app/schedtune.boost 0
    write /dev/stune/top-app/schedtune.prefer_idle 1
    write /dev/stune/top-app/schedtune.prefer_perf 0
    write /dev/stune/top-app/schedtune.util_est_en 1
    write /dev/stune/top-app/schedtune.ontime_en 1


    write /dev/stune/foreground/schedtune.boost 0
    write /dev/stune/foreground/schedtune.prefer_idle 0
    write /dev/stune/foreground/schedtune.prefer_perf 0
    write /dev/stune/foreground/schedtune.util_est_en 1
    write /dev/stune/foreground/schedtune.ontime_en 1

    write /dev/stune/background/schedtune.boost 0
    write /dev/stune/background/schedtune.prefer_idle 0
    write /dev/stune/background/schedtune.prefer_perf 0

    write /dev/stune/schedtune.boost 0

# CPUSET(s5e9925)
    chown system system /dev/cpuset/top-app/cpus
    chown system system /dev/cpuset/foreground/cpus
    chown system system /dev/cpuset/background/cpus
    chown system system /dev/cpuset/system-background/cpus
    chown system system /dev/cpuset/cpus
    chmod 664 /dev/cpuset/top-app/cpus
    chmod 664 /dev/cpuset/foreground/cpus
    chmod 664 /dev/cpuset/background/cpus
    chmod 664 /dev/cpuset/system-background/cpus
    chmod 664 /dev/cpuset/cpus
    write /dev/cpuset/top-app/cpus 0-7
    write /dev/cpuset/foreground/cpus 0-6
    write /dev/cpuset/background/cpus 0-2
    write /dev/cpuset/system-background/cpus 0-2

    mkdir /dev/cpuset/sf
    copy /dev/cpuset/cpus /dev/cpuset/sf/cpus
    copy /dev/cpuset/mems /dev/cpuset/sf/mems

    chown system system /dev/cpuset/sf/tasks
    chown system system /dev/cpuset/sf/cgroup.procs
    chown system system /dev/cpuset/sf/cpus

    chmod 0664 /dev/cpuset/sf/cpus
    chown system system /dev/cpuset/sf/cpus
    write /dev/cpuset/sf/cpus 0-6

    chown system system /dev/cpuset/tasks
    chown system system /dev/cpuset/cgroup.procs
    chmod 0664 /dev/cpuset/tasks

# foreground-boost
    mkdir /dev/cpuset/foreground-boost
    copy /dev/cpuset/cpus /dev/cpuset/foreground-boost/cpus
    copy /dev/cpuset/mems /dev/cpuset/foreground-boost/mems
    chown system system /dev/cpuset/foreground-boost
    chown system system /dev/cpuset/foreground-boost/tasks
    chown system system /dev/cpuset/foreground-boost/cgroup.procs
    chmod 0664 /dev/cpuset/foreground-boost/tasks
    chmod 0664 /dev/cpuset/foreground-boost/cgroup.procs

    chown system system /dev/cpuset/foreground/cpus
    chown system system /dev/cpuset/foreground-boost/cpus
    chmod 0664 /dev/cpuset/foreground/cpus
    chmod 0664 /dev/cpuset/foreground-boost/cpus
    copy /dev/cpuset/cpus /dev/cpuset/foreground-boost/cpus
    copy /dev/cpuset/mems /dev/cpuset/foreground-boost/mems
    write /dev/cpuset/foreground-boost/cpus 0-7

# set property to boost-optimize
    setprop sys.perf.boostopt true

    # MPAM
    chown system system /sys/kernel/mpam/control/late_init
    chmod 0664 /sys/kernel/mpam/control/late_init

    write /dev/cpuset/memory-intensive/cpus 0-6
    chown system system /sys/kernel/mpam/memory-intensive/cpbm0
    chmod 0664 /sys/kernel/mpam/memory-intensive/cpbm0
    chown system system /sys/kernel/mpam/memory-intensive/submit
    chmod 0664 /sys/kernel/mpam/memory-intensive/submit

    chown system system /sys/kernel/mpam/background/cpbm0
    chmod 0664 /sys/kernel/mpam/background/cpbm0
    chown system system /sys/kernel/mpam/background/submit
    chmod 0664 /sys/kernel/mpam/background/submit

    # EPIC interfaces
    chmod 660 /dev/stune/top-app/schedtune.boost
    chown system system /dev/mode
    chown system system /dev/exynos-migov
    chown system system /sys/devices/platform/exynos-migov/control/control_profile
    chown system system /sys/devices/platform/exynos-migov/control/set_margin
    chown system system /sys/devices/platform/exynos-migov/control/fragutil_thr
    chown system system /sys/devices/platform/exynos-migov/migov/running
    chown system system /dev/cpu_dma_latency
    chown system system /dev/cluster0_freq_min
    chown system system /dev/cluster1_freq_min
    chown system system /dev/cluster2_freq_min
    chown system system /dev/cluster0_freq_max
    chown system system /dev/cluster1_freq_max
    chown system system /dev/cluster2_freq_max
    chown system system /dev/cpu_online_min
    chown system system /dev/cpu_online_max
    chown system system /dev/gpu_freq_min
    chown system system /dev/gpu_freq_max
    chown system system /dev/bus_throughput
    chown system system /dev/bus_throughput_max
    chown system system /dev/device_throughput
    chown system system /dev/device_throughput_max
    chown system system /dev/intcam_throughput
    chown system system /dev/intcam_throughput_max
    chown system system /dev/display_throughput
    chown system system /dev/display_throughput_max
    chown system system /dev/cam_throughput
    chown system system /dev/cam_throughput_max
    chown system system /dev/npu_throughput
    chown system system /dev/npu_throughput_max
    chown system system /dev/aud_throughput
    chown system system /dev/aud_throughput_max
    chown system system /dev/dnc_throughput
    chown system system /dev/dnc_throughput_max
    chown system system /dev/mfc_throughput
    chown system system /dev/mfc_throughput_max
    chown system system /dev/csis_throughput
    chown system system /dev/csis_throughput_max
    chown system system /dev/isp_throughput
    chown system system /dev/isp_throughput_max
    chown system system /dev/mfc1_throughput
    chown system system /dev/mfc1_throughput_max
    chown system system /dev/dsp_throughput
    chown system system /dev/dsp_throughput_max
    chown system system /dev/alive_throughput
    chown system system /dev/alive_throughput_max
    chown system system /dev/vts_throughput
    chown system system /dev/vts_throughput_max
    chown system system /dev/hsi0_throughput
    chown system system /dev/hsi0_throughput_max
    chown system system /dev/ufd_throughput
    chown system system /dev/ufd_throughput_max
    chown system system /sys/kernel/gpu/gpu_ifpo_runtime_control

    # Apply network parameters for high data performance.
    write /proc/sys/net/core/rmem_default 327680
    write /proc/sys/net/core/rmem_max 8388608
    write /proc/sys/net/core/wmem_default 2805760
    write /proc/sys/net/core/wmem_max 8388608
    write /proc/sys/net/core/optmem_max 20480
    write /proc/sys/net/core/netdev_max_backlog 100000
    write /proc/sys/net/ipv4/tcp_rmem "2097152 4194304 8388608"
    write /proc/sys/net/ipv4/tcp_wmem "524288 1048576 4194304"
    write /proc/sys/net/ipv4/tcp_mem "44259 59012 88518"
    write /proc/sys/net/ipv4/udp_mem "88518 118025 177036"

    start watchdogd

# Accelerometer_sensor
    chown system radio /sys/class/sensors/accelerometer_sensor/raw_data
    chown system radio /sys/class/sensors/accelerometer_sensor/calibration
    chown system radio /sys/class/sensors/accelerometer_sensor/reactive_alert
